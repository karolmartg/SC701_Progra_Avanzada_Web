@model IEnumerable<CatalogViewModel>

@foreach (var c in Model)
{
    <div class="container">
        <div class="card">
            <div class="card-header">
                @c.Id
            </div>
            <div class="card-body">
                @c.Name
            </div>  
            <div class="card-body">
                @c.Modified
            </div>
        </div>
    </div>
}
